{
    "manifest_version": 2,
    "name": "YT Playlist Editor",
    "description": "A meaningful description to be implemented later",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "key": "----------",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background":{
        "scripts":[
            "./background.js",
            "./jsrsasign-all-min.js"
        ]
    },

    "permissions": [
        "tabs",
        "storage",
        "identity"
    ],

    "web_accessible_resources": ["libs/*"]
}